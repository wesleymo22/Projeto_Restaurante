<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="wwwroot\fonts\**" />
    <Content Remove="wwwroot\fonts\**" />
    <EmbeddedResource Remove="wwwroot\fonts\**" />
    <None Remove="wwwroot\fonts\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="wwwroot\css\animate.css" />
    <Content Remove="wwwroot\css\bootsnav.css" />
    <Content Remove="wwwroot\css\color.css" />
    <Content Remove="wwwroot\css\custom.css" />
    <Content Remove="wwwroot\css\flaticon.css" />
    <Content Remove="wwwroot\js\bootsnav.js" />
    <Content Remove="wwwroot\js\gmaps.min.js" />
